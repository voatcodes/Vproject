<%- include('partials/header') %>

<main class="access-point">
    <form action="/signup" method="post">
        <label for="">Date of Birth *</label>
        <div class="selectdob">
        <select class="selectdob" name="YYYY" id="year" onchange="change_year(this)"></select> 
        <select class="selectdob" id ="month" name = "mm" onchange="change_month(this)"></select> 
        <select class="selectdob" id ="day" name = "dd" ></select>
        </div>
        <label for="text">Full Name</label>
        <input type="text" name="fullname" id="" value="" required>
        <label for="">Prefered user name</label>
        <input type="text" name="preferedusername" id="" value="" required>
        <label for="text">Email Adress</label>
        <input type="email" name="email" id="" required>
        <label for="">Password</label>
        <input type="password" name="" id="" value="" required>
        <label for="">Confirm password</label>
        <input type="password" name="" id="" value="" required>
        <p>Already have an account? <a href="">Log In</a></p>
        <p>By signing in or creating an account, you agree with our <a>Terms & Conditions</a>  and <a>Privacy Statement</a></p>
        <div class="thingreyline"></div>
    </form>
</main>











