<%- include('../partials/header') %>

<main class="access-point">
    <form action="/users/signup" method="post">
        <label for="">Date of Birth *</label>
        <input type="date" name="dob" id="">
        <label for="text">Full Name *</label>
        <input type="text" name="fullname" id="" value="" required>
        <label for="">Prefered user name *</label>
        <input type="text" name="preferedUsername" id="" value="" required>
        <label for="text">Email Adress *</label>
        <input type="email" name="email" id="" value="" required>
        <label for="number">Phone number *</label>
        <input type="number" name="pNumber" id="" value="" required>
        <label for="">Password *</label>
        <input type="password" name="password" id="" value="" required>
        <label for="">Confirm password *</label>
        <input type="password" name="confirmPassword" id="" value="" required>
        <input type="submit" value="Sign up">
        <p>Already have an account? <a href="/users/login">Log In</a></p>
        <p>By signing in or creating an account, you agree with our <a>Terms & Conditions</a>  and <a>Privacy Statement</a></p>
        
    </form>
</main>














